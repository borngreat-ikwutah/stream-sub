{
  "name": "stream-sub",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "hardhat:compile": "node ./node_modules/.bin/hardhat compile",
    "hardhat:test": "node ./node_modules/.bin/hardhat test",
    "hardhat:node": "node ./node_modules/.bin/hardhat node",
    "deploy:local": "node ./node_modules/.bin/hardhat run scripts/deploy.js --network hardhat",
    "deploy:mantle": "node ./node_modules/.bin/hardhat run scripts/deploy.js --network mantleTestnet",
    "deploy:mantle-mainnet": "node ./node_modules/.bin/hardhat run scripts/deploy.js --network mantleMainnet",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "db:pull": "drizzle-kit pull",
    "db:reset": "drizzle-kit drop && drizzle-kit push",
    "db:seed": "tsx src/db/seed.ts",
    "db:up": "docker compose up -d postgres",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f postgres",
    "db:health": "tsx src/scripts/db-health.ts",
    "db:test": "tsx src/db/test-connection.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:restart": "docker compose restart",
    "docker:clean": "docker compose down -v && docker system prune -f",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\""
